'短正整'	"定义16位无符号短整数(uint16)类型"
'返回'		"返回(return)语句"
'句柄'		"定义句柄(HANDLE)类型"
'结构'		"定义结构(struct)类型"
'继续'		"继续(continue)语句"
'联合'		"定义联合(union)类型"
'逻辑'		"定义逻辑(bool)类型"
'枚举'		"定义枚举(enum)类型"
'取大小'	"取变量大小操作(sizeof)"
'取地址'	"取变量地址操作(addrof)"
'取数值'	"取变量数值操作(valueof)"
'双浮点'	"定义64位双精度浮点数(float64)类型"
'双精度'	"定义64位双精度浮点数(float64)类型"
'跳出'		"跳出(break)语句"
'外部的'	"定义外部的(extern)变量和函数"
